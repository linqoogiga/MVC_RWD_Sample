
@{
    ViewBag.Title = "CSS_Animation";
    Layout = null;
}

<style>
    div {
        position: absolute;
        left: 0;
        width: 50px;
        height: 50px;
        background: #AAAAAA;
        animation: 1s linear infinite progress-bar-stripes;
    }

    @@keyframes za {
        from {
            left: 0;
        }

        to {
            left: 100px;
        }
    }

    @@keyframes percent {
        0% {
            background: #fff;
        }

        25% {
            background: orange;
        }

        50% {
            background: #f00;
        }

        75% {
            background: #AAAAAA;
        }

        100% {
            background: #000;
        }
    }
</style>

<h2>CSS_Animation</h2>

<div></div>